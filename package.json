{
  "name": "@openinf/util-md-table",
  "version": "1.1.3",
  "description": "Common Markdown table-related utilities",
  "main": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "compile": "npm run compile-src && npm run compile-doc",
    "compile-src": "tsc --project ./",
    "compile-doc": "jsdoc2md --template README.hbs --files index.js > ./README.md",
    "test": "node index.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openinf/util-md-table.git"
  },
  "keywords": [
    "markdown",
    "json",
    "table",
    "utilities"
  ],
  "author": "The OpenINF Authors",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/openinf/util-md-table/issues"
  },
  "homepage": "https://github.com/openinf/util-md-table#readme",
  "devDependencies": {
    "@types/node": "16.11.27",
    "jsdoc-to-markdown": "7.1.1",
    "typescript": "4.6.3"
  }
}
