{
  "name": "@openinf/util-md-table",
  "version": "1.1.3",
  "description": "Common Markdown table-related utilities",
  "packageManager": "npm",
  "engines": {
    "node": "^16.13.0",
    "npm": "^8.1.0",
    "npx": "^8.1.0"
  },
  "main": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "prepare": "npm run compile-src && npm run compile-doc",
    "compile-src": "tsc --project ./",
    "compile-doc": "jsdoc2md --template README.hbs --files index.js > ./README.md",
    "test": "node index.spec.js"
  },
  "repository": "openinf/util-md-table",
  "keywords": ["markdown", "json", "table", "utilities"],
  "author": "The OpenINF Authors",
  "license": "MIT",
  "bugs": "https://github.com/openinf/util-md-table/issues",
  "homepage": "https://github.com/openinf/util-md-table#readme",
  "devDependencies": {
    "@types/node": "18.8.4",
    "jsdoc-to-markdown": "7.1.1",
    "typescript": "4.8.4"
  }
}
